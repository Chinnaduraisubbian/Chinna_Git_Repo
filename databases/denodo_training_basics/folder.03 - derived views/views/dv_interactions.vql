CREATE OR REPLACE VIEW dv_interactions FOLDER = '/03 - derived views' AS SELECT interaction_phone, customer_code, interaction_date, interaction_ip, case_id, agent_id FROM (SELECT customer_code, case_id, caller_phone AS interaction_phone, agent_id, date_call_received AS interaction_date FROM bv_cc_service_call UNION SELECT customer_ip AS interaction_ip, customer_code, sr_date AS interaction_date, global_case_id AS case_id, agent_id FROM bv_support_get_all_service_request);

